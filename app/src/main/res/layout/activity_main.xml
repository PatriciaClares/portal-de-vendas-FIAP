<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <ImageView
        android:id="@+id/mercadolivre"
        android:layout_width="116dp"
        android:layout_height="60dp"
        android:layout_marginStart="64dp"
        android:layout_marginTop="48dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/mercado_livre" />

    <ImageView
        android:id="@+id/ebay"
        android:layout_width="116dp"
        android:layout_height="64dp"
        android:layout_marginTop="36dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.216"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/mercadolivre"
        app:srcCompat="@drawable/ebay" />

    <ImageView
        android:id="@+id/amazon"
        android:layout_width="116dp"
        android:layout_height="60dp"
        android:layout_marginStart="52dp"
        android:layout_marginTop="48dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/mercadolivre"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/amazon" />

    <ImageView
        android:id="@+id/kabum"
        android:layout_width="116dp"
        android:layout_height="64dp"
        android:layout_marginStart="52dp"
        android:layout_marginTop="36dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/ebay"
        app:layout_constraintTop_toBottomOf="@+id/amazon"
        app:srcCompat="@drawable/kabum" />

    <ImageView
        android:id="@+id/wish"
        android:layout_width="116dp"
        android:layout_height="64dp"
        android:layout_marginStart="64dp"
        android:layout_marginTop="36dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/ebay"
        app:srcCompat="@drawable/wish" />

    <ImageView
        android:id="@+id/aliexpress"
        android:layout_width="116dp"
        android:layout_height="64dp"
        android:layout_marginStart="52dp"
        android:layout_marginTop="36dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/wish"
        app:layout_constraintTop_toBottomOf="@+id/kabum"
        app:srcCompat="@drawable/aliexpress" />

</androidx.constraintlayout.widget.ConstraintLayout>